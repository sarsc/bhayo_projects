

<div class="projects-container">
  <% @projects.each do |project| %>
    <div class="project-card">
      <div class= "bg">
        <div class="project-name">
          <%= link_to project.name, project_path(project), class: "link-project"%>
        </div>
        <% if user_signed_in? %>
        <%= link_to "delete", project_path(project), method: :delete %>
        <% end %>
      </div>

      <!-- cl_image_tag project.images.first.photo_url -->
    </div>
  <% end %>
</div>
